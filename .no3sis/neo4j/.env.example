# Synapse System Environment Configuration
# Copy this to .env and modify as needed

# Neo4j Configuration
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=synapse_neo4j_pass

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

